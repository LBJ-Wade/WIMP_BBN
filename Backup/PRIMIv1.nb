    RowBox[{
     FractionBox["\[Alpha]FS", "\[Pi]"], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"kB", " ", "T"}], ")"}], "4"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         FractionBox["2", "3"]}], 
