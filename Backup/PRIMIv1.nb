         RowBox[{
          RowBox[{"ImnT", "[", "1", "]"}], "[", 
          RowBox[{"0", ",", "1"}], "]"}], "[", "T", "]"}]}], "-", 
       RowBox[{
        FractionBox["2", 
         SuperscriptBox["\[Pi]", "2"]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"ImnT", "[", "1", "]"}], "[", 
            RowBox[{"0", ",", "1"}], "]"}], "[", "T", "]"}], ")"}], "2"]}]}], 
      ")"}]}]}]}], ";"}]], "Input",
