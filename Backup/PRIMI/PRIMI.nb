            SuperscriptBox["p2", "2"]}], 
           RowBox[{"p1", " ", "p2", " ", "e1", " ", "e2"}]], " ", 
          RowBox[{"Log", "[", 
           RowBox[{"Abs", "[", 
            FractionBox[
             RowBox[{"(", 
              RowBox[{"p1", "+", "p2"}], ")"}], 
             RowBox[{"(", 
              RowBox[{"p1", "-", "p2"}], ")"}]], "]"}], "]"}], 
          FractionBox["1", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Exp", "[", "e1", "]"}], "+", "1"}], ")"}], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"Exp", "[", "e2", "]"}], "+", "1"}], ")"}]}]]}]}], 
        "\[IndentingNewLine]", "]"}], "]"}], ",", 
      RowBox[{"\"\<RuntimeOptions\>\"", "\[Rule]", "\"\<Speed\>\""}], ",", 
