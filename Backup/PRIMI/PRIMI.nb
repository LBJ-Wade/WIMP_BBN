Cell[BoxData[{
 RowBox[{"Clear", "[", "Imn", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Imn", "[", "sgn_", "]"}], "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], "[", "x_", "]"}], ":=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["pe", "2"], "+", 
         SuperscriptBox["x", "2"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", "-", "1"}], ")"}], "/", "2"}], ")"}]], 
      SuperscriptBox["pe", 
       RowBox[{"(", 
        RowBox[{"n", "+", "1"}], ")"}]]}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Exp", "[", 
        SqrtBox[
         RowBox[{
          SuperscriptBox["pe", "2"], "+", 
          SuperscriptBox["x", "2"]}]], "]"}], "+", "sgn"}], ")"}]], ",", 
    RowBox[{"{", 
     RowBox[{"pe", ",", "0", ",", "Infinity"}], "}"}], ",", 
    RowBox[{"Method", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"Automatic", ",", 
       RowBox[{"\"\<SymbolicProcessing\>\"", "\[Rule]", "0"}]}], "}"}]}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"ImnT", "[", "sgn_", "]"}], "[", 
     RowBox[{"m_", ",", "n_"}], "]"}], "[", "T_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"Imn", "[", "sgn", "]"}], "[", 
     RowBox[{"m", ",", "n"}], "]"}], "[", 
    FractionBox["me", 
     RowBox[{"kB", " ", "T"}]], "]"}]}], "\n", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", "Interpolations", " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ImnI", "[", "sgn_", "]"}], "[", 
