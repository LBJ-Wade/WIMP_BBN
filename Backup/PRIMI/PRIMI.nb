Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"DumpMC", "[", "File_String", "]"}], ":=", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/MC\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"\"\<MonteCarlo/MC\>\"", "<>", "File", "<>", "\"\<.dat\>\""}],
         ",", "MC"}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/RV\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"\"\<MonteCarlo/RV\>\"", "<>", "File", "<>", "\"\<.dat\>\""}],
         ",", "RV"}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/Cosmo\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
