          RowBox[{"1.", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"b", "-", "a"}], ")"}], "/", "Np"}]}]}], ",", 
         RowBox[{"n2", "=", 
          RowBox[{"Np", "/", "2"}]}]}], "}"}], ",", 
