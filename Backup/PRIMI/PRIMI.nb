      RowBox[{"Evaluate", "[", 
       RowBox[{"With", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"e1", "=", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["p1", "2"], "+", 
              SuperscriptBox["x", "2"]}]]}], ",", 
           RowBox[{"e2", "=", 
            SqrtBox[
             RowBox[{
              SuperscriptBox["p2", "2"], "+", 
              SuperscriptBox["x", "2"]}]]}]}], "}"}], ",", 
