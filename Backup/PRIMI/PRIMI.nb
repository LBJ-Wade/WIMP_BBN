Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Eta]factorT", "[", "Tv_", "]"}], ":=", 
   RowBox[{
    RowBox[{"nB", "[", 
     RowBox[{"a", "[", "Tv", "]"}], "]"}], "*", 
    FractionBox[
     SuperscriptBox["\[Pi]", "2"], 
     RowBox[{"2", 
      RowBox[{"Zeta", "[", "3", "]"}]}]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"hbar", " ", "clight"}], 
       RowBox[{"kB", " ", "Tv"}]], ")"}], "3"]}]}], ";"}]], "Input",
