  3.718728355516646*^9, 3.7187283583181467`*^9}, {3.718728429014056*^9, 
  3.718728429249858*^9}, {3.718786116517528*^9, 3.7187861257199287`*^9}, {
  3.7187864466005*^9, 3.718786450321663*^9}, {3.718787302865569*^9, 
