        RowBox[{
         RowBox[{
          RowBox[{"Imn", "[", "sgn", "]"}], "[", 
          RowBox[{"m", ",", "n"}], "]"}], "[", 
         FractionBox["me", 
          RowBox[{"kB", " ", "Tv"}]], "]"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Tv", ",", "ListT"}], "}"}]}], 
     "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
