       RowBox[{"kB", " ", "T"}], "me"], ")"}], "2"], 
    RowBox[{"(", 
     RowBox[{
      FractionBox[
       RowBox[{"2", "\[Pi]", " ", "\[Alpha]FS"}], "3"], "+", " ", 
      RowBox[{
       FractionBox[
        RowBox[{"4", "\[Alpha]FS"}], "\[Pi]"], " ", 
       RowBox[{
        RowBox[{
         RowBox[{"ImnT", "[", "1", "]"}], "[", 
         RowBox[{"0", ",", "1"}], "]"}], "[", "T", "]"}]}]}], ")"}]}]}], 
  RowBox[{"(*", " ", 
   RowBox[{
   "Only", " ", "main", " ", "part", " ", "of", " ", "mass", " ", "shift"}], 
   " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"dm\[Gamma]2", "[", "T_", "]"}], ":=", " ", 
  RowBox[{
   FractionBox[
    RowBox[{"8", "\[Alpha]FS"}], "\[Pi]"], " ", 
   RowBox[{
    RowBox[{
     RowBox[{"ImnT", "[", "1", "]"}], "[", 
     RowBox[{"0", ",", "1"}], "]"}], "[", "T", "]"}], 
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"kB", " ", "T"}], "me"], ")"}], "2"]}]}]}], "Input",
