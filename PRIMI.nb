       RowBox[{"{", 
        RowBox[{"Np", ",", "_Integer"}], "}"}]}], "}"}], ",", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"h", "=", 
          RowBox[{"1.", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"b", "-", "a"}], ")"}], "/", "Np"}]}]}], ",", 
         RowBox[{"n2", "=", 
          RowBox[{"Np", "/", "2"}]}]}], "}"}], ",", 
       RowBox[{"With", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"h3", "=", 
           RowBox[{"h", "/", "3."}]}], "}"}], ",", 
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"a", ",", "h3"}], "}"}], "}"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"a", "+", 
