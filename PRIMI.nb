Cell["\<\
We define functions to dump the results of a Monte - Carlo in a file and also \
the converse to load the result so as to analyze and use it.\
\>", "Text",
 CellChangeTimes->{{3.722687892754711*^9, 3.722687913027423*^9}}],

Cell[BoxData[
 RowBox[{"Clear", "[", 
  RowBox[{"LoadMC", ",", "DumpMC"}], "]"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.720344641965357*^9, 3.720344645338798*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"DumpMC", "[", "File_String", "]"}], ":=", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/MC\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"\"\<MonteCarlo/MC\>\"", "<>", "File", "<>", "\"\<.dat\>\""}],
         ",", "MC"}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/RV\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{"\"\<MonteCarlo/RV\>\"", "<>", "File", "<>", "\"\<.dat\>\""}],
         ",", "RV"}], "]"}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<Exporting \>\"", ",", 
        RowBox[{
        "\"\<MonteCarlo/Cosmo\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Export", "[", 
       RowBox[{
        RowBox[{
        "\"\<MonteCarlo/Cosmo\>\"", "<>", "File", "<>", "\"\<.dat\>\""}], ",",
         "Cosmo"}], "]"}], ";"}], ")"}]}], ";"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LoadMC", "[", "File_String", "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"MCfile", "=", 
      RowBox[{"\"\<MonteCarlo/MC\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"RVfile", "=", 
      RowBox[{"\"\<MonteCarlo/RV\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"Cosmofile", "=", 
      RowBox[{
      "\"\<MonteCarlo/Cosmo\>\"", "<>", "File", "<>", "\"\<.dat\>\""}]}], ";",
      "\n", "        ", 
     RowBox[{"MC", "=", 
      RowBox[{"Import", "[", "MCfile", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"RV", "=", 
      RowBox[{"Import", "[", "RVfile", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Cosmo", "=", 
      RowBox[{"Import", "[", "Cosmofile", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"TMC", "=", 
      RowBox[{"Transpose", "[", "MC", "]"}]}], ";"}], "\[IndentingNewLine]", 
    ")"}]}], ";"}]}], "Input",
 InitializationCell->True],

