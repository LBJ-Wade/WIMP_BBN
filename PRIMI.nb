               RowBox[{"2", " ", "h3"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", 
               RowBox[{"n2", "-", "1"}]}], "}"}]}], "]"}], ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"a", "+", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"2.", " ", "j"}], "-", "1"}], ")"}], " ", "h"}]}], 
               ",", 
               RowBox[{"4", " ", "h3"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", "1", ",", "n2"}], "}"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"b", ",", "h3"}], "}"}], "}"}]}], "]"}]}], "]"}]}], 
      "]"}], ",", 
