 InitializationCell->True],

Cell[BoxData[
 RowBox[{
  RowBox[{"FillReactionMatrix", "[", "listreac_List", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "Tab", ",", "i", ",", "j", ",", "nvar", ",", "TreatReaction", ",", 
      "FactorInitialElements"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"nvar", "=", 
      RowBox[{"Length", "@", "VarList"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Tab", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"ii", ",", "1", ",", "nvar"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"FactorInitialElements", "[", "el_List", "]"}], ":=", 
      RowBox[{"Times", "@@", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"A\[Rho]B", "/", "DensityUnit"}], " ", 
              RowBox[{"Y", "[", 
               RowBox[{"KeyVal", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}]}], ")"}], "^", 
            RowBox[{"#", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "/", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "!"}], ")"}]}], "&"}], "/@", 
         "el"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"TreatReaction", "[", "reaction_List", "]"}], ":=", 
      RowBox[{"Module", "[", 
