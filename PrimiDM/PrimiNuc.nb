         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Tab", "[", 
              RowBox[{"[", 
               RowBox[{"KeyVal", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}], "]"}], "=", 
             RowBox[{
              RowBox[{"Tab", "[", 
               RowBox[{"[", 
                RowBox[{"KeyVal", "[", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}], "]"}], "-", 
              RowBox[{"ReactionForward", " ", "FactorInitialForward", " ", 
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "2", "]"}], "]"}], "/", "A\[Rho]B"}], "*", 
               "DensityUnit"}]}]}], ")"}], "&"}], "/@", "InitialParticles"}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Tab", "[", 
              RowBox[{"[", 
               RowBox[{"KeyVal", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}], "]"}], "=", 
             RowBox[{
              RowBox[{"Tab", "[", 
               RowBox[{"[", 
                RowBox[{"KeyVal", "[", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], "]"}], "]"}], "]"}], "+", 
              RowBox[{"ReactionForward", " ", "FactorInitialForward", " ", 
               RowBox[{
