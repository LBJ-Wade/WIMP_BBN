Cell[BoxData[
 RowBox[{
  RowBox[{"YNSE", "[", 
   RowBox[{"name_", ",", "Yn_", ",", "Yp_", ",", "Tv_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "Pair", ",", "N", ",", "A", ",", "Z", ",", "mN", ",", "A32Overmn"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"mN", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"mn", "+", "mp"}], ")"}], "/", "2"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Pair", "=", 
      RowBox[{"KeySpecies", "[", "name", "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"Z", "=", 
      RowBox[{"Pair", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"N", "=", 
      RowBox[{"Pair", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"A", "=", 
      RowBox[{"Z", "+", "N"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"A32Overmn", "=", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"Mass", "[", "name", "]"}], 
         RowBox[{
          SuperscriptBox["mn", 
           RowBox[{"A", "-", "Z"}]], "*", 
          SuperscriptBox["mp", "Z"]}]], ")"}], 
       RowBox[{"3", "/", "2"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "*", 
         RowBox[{"SpinKeys", "[", "name", "]"}]}], "+", "1"}], ")"}], 
      SuperscriptBox[
       RowBox[{"Zeta", "[", "3", "]"}], 
       RowBox[{"(", 
        RowBox[{"A", "-", "1"}], ")"}]], 
      SuperscriptBox["\[Pi]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "A"}], ")"}], "/", "2"}], ")"}]], 
      SuperscriptBox["2", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"3", "A"}], "-", "5"}], ")"}], "/", "2"}], ")"}]], 
      "A32Overmn", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"kB", " ", "Tv"}], ")"}], 
       RowBox[{"3", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"A", "-", "1"}], ")"}], "/", "2"}]}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Eta]factorT", "[", "Tv", "]"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"A", "-", "1"}], ")"}]], 
      SuperscriptBox["Yp", "Z"], 
      SuperscriptBox["Yn", 
       RowBox[{"A", "-", "Z"}]], 
      RowBox[{"Exp", "[", 
       FractionBox[
        RowBox[{
         RowBox[{"BindingEnergy", "[", "name", "]"}], "*", "keV"}], 
        RowBox[{"kB", " ", "Tv"}]], "]"}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
