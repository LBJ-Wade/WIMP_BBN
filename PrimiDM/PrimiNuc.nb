Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CheckReaction", "[", 
    RowBox[{"{", 
     RowBox[{"ListIn_", ",", "ListOut_"}], "}"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Znet", ",", "Nnet", ",", "Anet"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Znet", "=", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Plus", "@@", 
          RowBox[{"(", 
           RowBox[{"Zi", "/@", "ListIn"}], ")"}]}]}], "+", 
        RowBox[{"Plus", "@@", 
         RowBox[{"(", 
          RowBox[{"Zi", "/@", "ListOut"}], ")"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Nnet", "=", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Plus", "@@", 
          RowBox[{"(", 
           RowBox[{"Ni", "/@", "ListIn"}], ")"}]}]}], "+", 
        RowBox[{"Plus", "@@", 
         RowBox[{"(", 
          RowBox[{"Ni", "/@", "ListOut"}], ")"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Anet", "=", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Plus", "@@", 
          RowBox[{"(", 
           RowBox[{"Ai", "/@", "ListIn"}], ")"}]}]}], "+", 
        RowBox[{"Plus", "@@", 
         RowBox[{"(", 
          RowBox[{"Ai", "/@", "ListOut"}], ")"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{
         "ListIn", ",", "\"\< \>\"", ",", "ListOut", ",", "\"\< \>\"", ",", 
          "Znet", ",", "Nnet", ",", "Anet"}], "]"}], ";"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Znet", "=!=", "0"}], "||", 
         RowBox[{"Nnet", "=!=", "0"}], "||", 
         RowBox[{"Anet", "=!=", "0"}]}], ",", "\[IndentingNewLine]", 
