  3.718337314754148*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"SolveValueMiddleTemperatures", ":=", 
   RowBox[{"(", 
    RowBox[{"If", "[", 
     RowBox[{"$CompileNDSolve", ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Compiled", " ", 
        RowBox[{"version", "."}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"resMT", "=", 
        RowBox[{"NDSolveValue", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Ytab", "'"}], "[", "tv", "]"}], "==", 
             RowBox[{"DY18CN", "[", 
              RowBox[{
               RowBox[{"\[Rho]BForBBN", "@", 
                RowBox[{"a", "@", 
                 RowBox[{"Toft", "@", "tv"}]}]}], ",", 
               RowBox[{"Rules\[Lambda]RHS18I", "[", 
                RowBox[{"Toft", "@", "tv"}], "]"}], ",", 
               RowBox[{"Rules\[Lambda]barRHS18I", "[", 
                RowBox[{"Toft", "@", "tv"}], "]"}], ",", 
               RowBox[{"Ytab", "[", "tv", "]"}]}], "]"}]}], ",", 
            RowBox[{
             RowBox[{"Ytab", "[", "tmiddle", "]"}], "==", 
             RowBox[{
              RowBox[{"YPeriodTimeOrStateEquilibrium", "[", 
               RowBox[{"\"\<HT\>\"", ",", "ListThermalValuesUsedMT"}], "]"}], 
              "[", 
              RowBox[{"TMiddle", ",", "tmiddle"}], "]"}]}]}], "}"}], ",", 
          "\[IndentingNewLine]", "Ytab", ",", 
          RowBox[{"{", 
           RowBox[{"tv", ",", "tmiddle", ",", "t18"}], "}"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"Method", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"\"\<BDF\>\"", ",", 
             RowBox[{"\"\<MaxDifferenceOrder\>\"", "\[Rule]", "$BDFOrder"}]}],
             "}"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"PrecisionGoal", "\[Rule]", 
           RowBox[{"7", "+", "PrecisionNDSolve"}]}], ",", 
          RowBox[{"AccuracyGoal", "\[Rule]", "11"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"InterpolationOrder", "\[Rule]", "InterpOrder"}], ",", 
          RowBox[{"Compiled", "\[Rule]", "Automatic"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Y", "[", "\"\<MT\>\"", "]"}], "[", "key_", "]"}], "[", 
         RowBox[{"tv_", "?", "NumericQ"}], "]"}], ":=", 
        RowBox[{
         RowBox[{"resMT", "[", "tv", "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"KeyVal", "[", "key", "]"}], "]"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"tMT", "=", 
