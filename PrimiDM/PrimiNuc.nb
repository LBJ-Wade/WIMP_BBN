Cell["\<\
This function constructs the reverse reaction. Its arguments are the name of \
the reverse reaction, the front factor, the power on T9 and the Q of the \
reaction.\
\>", "Text",
 CellChangeTimes->{{3.699358780002651*^9, 3.6993587892577677`*^9}, {
  3.7159425783968897`*^9, 3.7159426096370077`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"ReverseReaction", "[", 
     RowBox[{
     "Name_", ",", "FrontFactor_", ",", "PoweronT9_", ",", "Qoverkb_"}], 
     "]"}], ":=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Reversname", "=", 
         RowBox[{"ToExpression", "[", 
          RowBox[{"\"\<Hold@Lbar\>\"", "<>", "Name"}], "]"}]}], ",", 
        RowBox[{"name", "=", 
         RowBox[{"Evaluate", "[", 
          RowBox[{"Symbol", "[", 
           RowBox[{"\"\<L\>\"", "<>", "Name"}], "]"}], "]"}]}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"FrontFactor", ">", "0"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"MySet", "[", 
          RowBox[{"Reversname", ",", 
           RowBox[{"Function", "[", 
            RowBox[{
             RowBox[{"{", "Tvr", "}"}], ",", 
             RowBox[{"With", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"T9", "=", 
                 RowBox[{"Tvr", "/", "Giga"}]}], "}"}], ",", 
               RowBox[{"FrontFactor", 
                SuperscriptBox[
                 RowBox[{"(", "T9", ")"}], "PoweronT9"], "*", 
                RowBox[{"Exp", "[", 
                 FractionBox["Qoverkb", "T9"], "]"}], "*", 
                RowBox[{"name", "[", "Tvr", "]"}]}]}], "]"}]}], "]"}]}], 
          "]"}], ";"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"MySet", "[", 
          RowBox[{"Reversname", ",", 
           RowBox[{"Function", "[", 
            RowBox[{
             RowBox[{"{", "Tvr", "}"}], ",", "0"}], "]"}]}], "]"}], ";"}]}], 
