Cell["\<\
We define an interpolation of the results. We join the results from high, \
middle and low temperature eras. This is joined in the function 

YI[\[OpenCurlyDoubleQuote]key\[CloseCurlyDoubleQuote]][time] where key is the \
name of the nuclide (e.g. \[OpenCurlyDoubleQuote]a\[CloseCurlyDoubleQuote] \
for He4, \[OpenCurlyDoubleQuote]t\[CloseCurlyDoubleQuote] for tritium and \
\[OpenCurlyDoubleQuote]d\[CloseCurlyDoubleQuote] for deuterium but otherwise \
\[OpenCurlyDoubleQuote]Li7\[CloseCurlyDoubleQuote], \
\[OpenCurlyDoubleQuote]C12\[CloseCurlyDoubleQuote] etc...).\
\>", "Text",ExpressionUUID->"eeb4411c-2b31-4012-a849-42ac8459d3ae"],

Cell[BoxData[
 RowBox[{
  RowBox[{"InterpolateResults", "=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Clear", "[", 
      RowBox[{"Yall", ",", "YI"}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Yall", "[", 
       RowBox[{"key_", "?", "KeyQ"}], "]"}], ":=", 
      RowBox[{
       RowBox[{"Yall", "[", "key", "]"}], "=", 
       RowBox[{"Function", "[", 
        RowBox[{
         RowBox[{"{", "tv", "}"}], ",", 
         RowBox[{"Piecewise", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Y", "[", "\"\<HT\>\"", "]"}], "[", "key", "]"}], "[",
                "tv", "]"}], ",", 
              RowBox[{"tv", "<", "tmiddle"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Y", "[", "\"\<MT\>\"", "]"}], "[", "key", "]"}], "[",
                "tv", "]"}], ",", 
              RowBox[{
               RowBox[{"tv", "<", "t18"}], "&&", 
               RowBox[{"tv", ">=", "tmiddle"}]}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"Y", "[", "\"\<LT\>\"", "]"}], "[", "key", "]"}], "[",
                "tv", "]"}], ",", 
              RowBox[{
               RowBox[{"tv", "<=", "tend"}], "&&", 
               RowBox[{"tv", ">=", "t18"}]}]}], "}"}]}], "}"}], "]"}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"YI", "[", 
       RowBox[{"key_", "?", "KeyQ"}], "]"}], ":=", 
      RowBox[{
       RowBox[{"YI", "[", "key", "]"}], "=", 
       RowBox[{"Interpolation", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"tv", ",", 
             RowBox[{
              RowBox[{"Yall", "[", "key", "]"}], "[", "tv", "]"}]}], "}"}], 
           ",", 
           RowBox[{"{", 
            RowBox[{"tv", ",", 
             RowBox[{"Join", "[", 
              RowBox[{"tHT", ",", 
               RowBox[{"Rest", "@", "tMT"}], ",", 
               RowBox[{"Rest", "@", "tLT"}]}], "]"}]}], "}"}]}], "]"}], ",", 
         RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}]}], ";"}], 
    ")"}]}], ";"}]], "Input",
 InitializationCell->True,
