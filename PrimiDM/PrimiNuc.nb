d0e11bf2cf8c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SolveValueMiddleTemperatures", ":=", 
   RowBox[{"(", 
    RowBox[{"If", "[", 
     RowBox[{"$CompileNDSolve", ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Compiled", " ", 
        RowBox[{"version", "."}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"resMT", "=", 
        RowBox[{"NDSolveValue", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Ytab", "'"}], "[", "tv", "]"}], "==", 
             RowBox[{"DY18CN", "[", 
              RowBox[{
               RowBox[{"\[Rho]BForBBN", "@", 
                RowBox[{"a", "@", 
                 RowBox[{"Toft", "@", "tv"}]}]}], ",", 
               RowBox[{"Rules\[Lambda]RHS18I", "[", 
                RowBox[{"Toft", "@", "tv"}], "]"}], ",", 
               RowBox[{"Rules\[Lambda]barRHS18I", "[", 
                RowBox[{"Toft", "@", "tv"}], "]"}], ",", 
               RowBox[{"Ytab", "[", "tv", "]"}]}], "]"}]}], ",", 
            RowBox[{
             RowBox[{"Ytab", "[", "tmiddle", "]"}], "==", 
             RowBox[{
              RowBox[{"YPeriodTimeOrStateEquilibrium", "[", 
               RowBox[{"\"\<HT\>\"", ",", "ListThermalValuesUsedMT"}], "]"}], 
              "[", 
              RowBox[{"TMiddle", ",", "tmiddle"}], "]"}]}]}], "}"}], ",", 
